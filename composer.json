{
    "name": "corex/lmodel",
    "type": "library",
    "description": "Laravel Model (generator, constants, preserved lines, phpdoc)",
    "license": "MIT",
    "keywords": [
        "laravel",
        "model",
        "generator"
    ],
    "authors": [
        {
            "name": "CoRex",
            "email": "dev@corex.dk"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "laravel/framework": "^7.0",
        "doctrine/dbal": "^2.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4|^9.0",
        "corex/helpers": "^2.4",
        "corex/debug": "^2.0",
        "orchestra/testbench": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "CoRex\\Laravel\\Model\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\CoRex\\Laravel\\Model\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CoRex\\Laravel\\Model\\ModelServiceProvider"
            ]
        }
    }
}
